@model Dictionary<string,string>
@using Personal.Domain.Enums
@{
    var startDate = new System.DateTime(2016, 01, 01);
    var yearsOfWorking = DateTime.Now.Year - startDate.Year;
    if (startDate > DateTime.Now.AddYears(-yearsOfWorking))
        yearsOfWorking--;
}

<section class="about-me-section p-3 p-lg-5 theme-bg-light" id="about-me">
    <div class="container">
        <div class="profile-teaser row">

            <div class="col">
                <h2 class="name font-weight-bold mb-1">@Model[AppSettingKeys.Name.ToString()]</h2>
                <div class="tagline mb-3">@Model[AppSettingKeys.Position.ToString()]</div>
                <div class="bio mb-4">
                    @Model[AppSettingKeys.ContentBio.ToString()]
                </div>
                <div class="mb-4">
                    <a class="btn btn-primary me-2 mb-3" href="/attachment/resume" target="_blank"><i class="fas fa-arrow-alt-circle-right me-2"></i><span class="d-none d-md-inline">View</span> Resume</a>
                </div>
            </div><!--//col-->

            <div class="col-md-5 col-lg-5">
            </div>
        </div>
    </div>
</section>

<section class="overview-section p-3 p-lg-5" id="portfolio">
    <div class="container">
        <h2 class="section-title font-weight-bold mb-3">What I do</h2>
        <div class="section-intro mb-5">I have more than  @yearsOfWorking years' experience building software. Below is a quick overview of my main technical skill sets and technologies I use. Want to find out more about my experience? Check out my <a class="text-link" href="/attachment/resume">resume</a>.</div>
        <div id="skills-container">
        </div>
    </div>
</section>


<section class="latest-blog-section p-3 p-lg-5" id="blogs">
    <div class="container">
        <h2 class="section-title font-weight-bold mb-5">Latest Blog Posts</h2>
        <div id="blog-content-container">
        </div>
    </div>
</section>

<section class="my-work-section p-3 p-lg-5" id="my-works">
    <div class="container">
        <h2 class="section-title font-weight-bold mb-5">My Works</h2>
        <div class="bio mb-4">
            Below are some repositories hosted on GitHub.
        </div>
        <div class="theme-bg-light" id="my-works-container">
            <h3 style="height:200px;text-align:center;line-height:200px;vertical-align:middle">Section Under Construction....</h3>
        </div>

    </div>
</section>

<section class="p-3 p-lg-5" id="contact">
    <div class="container">
        <h2 class="section-title font-weight-bold mb-5">Contact</h2>

        @{
            Html.RenderPartialAsync("~/Views/Contact/_ContactForm.cshtml");
        }
    </div>
</section>

@section scripts{
<script src="~/js/dashboard.js" asp-append-version="true"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js" integrity="sha512-UdIMMlVx0HEynClOIFSyOrPggomfhBKJE28LKl8yR3ghkgugPnG6iLfRfHwushZl1MOPSY6TsuBDGPK2X4zYKg==" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-ajax-unobtrusive/3.2.6/jquery.unobtrusive-ajax.min.js" integrity="sha512-DedNBWPF0hLGUPNbCYfj8qjlEnNE92Fqn7xd3Sscfu7ipy7Zu33unHdugqRD3c4Vj7/yLv+slqZhMls/4Oc7Zg==" crossorigin="anonymous"></script>


<recaptcha-script />
}